{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "headers": [{
      "source": "index.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/javascript;charset=utf-8"
        }
      ]
    }, {
      "source": "**/resources/*.png",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/png"
        }
      ]
    }, {
      "source": "**/*.png",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/png"
        }
      ]
    }, {
      "source": "**",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }],
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
